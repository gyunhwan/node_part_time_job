          <td colspan="2"><input style="width:100%" type="text" name="level_of_education_13" value="<%=data2[0].level_of_education_13%>" ></td>
        </tr>
        <tr>
                <td><input type="date" style="width:100%" name="level_of_education_21" value="<%=data2[0].level_of_education_21%>" ></td>
                <td colspan="2"><input style="width:100%"colspan="4" type="text" name="level_of_education_22" value="<%=data2[0].level_of_education_22%>"></td>
        
                <td colspan="2"><input style="width:100%" type="text" name="level_of_education_23" value="<%=data2[0].level_of_education_23%>"></td>
              </tr>
        <tr>
  
            <td><input type="date" name="level_of_education_31" style="width:100%" value="<%=data2[0].level_of_education_31%>"></td>
  
  
          <td colspan="2"><input style="width:100%" type="text" name="level_of_education_32" value="<%=data2[0].level_of_education_32%>" ></td>
  
          <td colspan="2"><input style="width:100%" type="text" name="level_of_education_33" value="<%=data2[0].level_of_education_33%>" ></td>
  
