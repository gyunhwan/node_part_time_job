          <td style="width: 30%"><input type="text" name="registration_tel" value="<%=data[0].registration_tel%>" ></td>
          <th  rowspan="1" style="background-color:#ECF6CE">email</th>
          <td colspan="2"  rowspan="1"><input type="text" name="registration_email" value="<%=data[0].registration_email%>" ></td>
        </tr>
  
        <tr>
            <th style="background-color:#ECF6CE">집</th>
            <td colspan="6"> <input style="width:100%;" type="text" name="registration_home" value="<%=data[0].registration_home%>" ></td>
  
        </tr>
  
        <tr>
          <th style="background-color:#F7F8E0"  >졸업연도</th>
          <th style="width: 50%;background-color:#F7F8E0" colspan="2">학력 및 경력 사항</th>
  
          <th style="width: 30%;background-color:#F7F8E0" colspan="2">비고</th>
  
        </tr>
  
        <tr>
          <td><input type="date" style="width:100%" name="level_of_education_11" value="<%=data2[0].level_of_education_11%>" ></td>
