<!--이력서수정화면-->
<div style="height:3000px"> 
    <h1 style="text-align:center" >이력서</h1>
    <form action="applicationupdate" method="post" enctype="application/json" style="height:100%">
      <table border="1" style="width: 60%;height:30%" align="center">
        <tr>
          <th style="width: 20%;background-color:#F7F8E0" rowspan="1">성명</th>
  
          <td rowspan="1" colspan="6"><input name="registration_name" style="width:85%" type="text" value="<%=data[0].registration_name%>"> (인)</td>
        </tr>
  
        </tr> 
        <tr>
            <th colspan="1"  style="background-color:#F7F8E0">주민등록번호</th>
              <td colspan="6" style="width: 100%" colspan="2"><input style="width:100%" name="registration_number" value="<%=data[0].registration_number%>" placeholder="xxxxxx-xxxxxxx" type="text">
              </td>
       
        <tr>
            <th style="width: 30%;background-color:#F7F8E0" >생년월일</th>
            <td style="width: 100%" colspan="6"><input type="text" name="year" value="<%=data[0].registration_year%>"> 년
            <input name="month" type="text" value="<%=data[0].registration_month%>" >월 <input type="text" name="day" value="<%=data[0].registration_day%>"> 일
            (만 <input type="text" name="registration_old" value="<%=data[0].registration_old%>" > 세)</td>
        </tr>
        <tr>
          <th style="width: 30%;background-color:#F7F8E0" colspan="1">주소</th>
